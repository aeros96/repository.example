<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.smartnetwizard" name="SmartNet Wizard" version="1.3" provider-name="SmartNet">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">SmartNet</summary>
    <description lang="en">Builds by SmartNet FOR friends</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.smartIPTV"
       name="[COLOR white][B][COLOR dodgerblue]SmartNet IPTV[/B][/COLOR]"
       version="1.0.2"
       provider-name="Shark55">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
	<import addon="script.module.resolveurl" version="5.0.00"/>
    <import addon="script.module.urlresolver" />
    <import addon="script.module.simplejson" />
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>

  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en"></summary>
    <description lang="en">[COLOR dodgerblue]Sports Classic Don't host any servers  nor streams  all content shared here is freely available all over the web [/COLOR] Peace !</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.smartnet9" name="smartnet_repo" version="1.1" provider-name="Aeros">
    <extension point="xbmc.addon.repository" name="smartnet_repo">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/aeros96/smartnet_repo/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/aeros96/smartnet_repo/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/aeros96/smartnet_repo/master/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>smartnet_repo</summary>
        <description>SmartNet Test Repo</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
